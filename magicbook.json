  "javascripts" : {
    "bundle" : true,
    "compress" : true,
    "digest" : true,
    "files" : [
      "javascripts/srcdoc-polyfill.js",
      "javascripts/rune.js",
      "javascripts/utils.js",
      "javascripts/p5.js",
      "javascripts/splash.js"
    ]
  },
  "builds" : [
    {
      "format" : "html"
    }
  ],
  "codesplit" : {
    "includes" : "examples"
  },
  "layout" : "layouts/default.html"
}
