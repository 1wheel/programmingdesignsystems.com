{
  "title" : "Programming Design Systems",
  "author" : "Rune Madsen",
  "twitter" : "https://twitter.com/runemadsen",
  "permalink" : ":parts/:title/index.html",
  "addPlugins" : [
    "magicbook-codesplit",
    "./plugins/p5",
    "./plugins/highlight"
  ],
  "files" : [
    "content/index.html",
    "content/2-introduction.html",
    {
      "label" : "Shape",
      "files" : [ "content/shape/*.html" ],
      "colors" : {
        "link" : "#FFDAA2",
        "codeBackground" : "#FFFFE2",
        "codeText" : "#726C54",
        "codeVariable" : "#E1B000",
        "codeStringNum" : "#acb835",
        "codeKeyword" : "#C17C2D"
      }
    }
  ],
  "stylesheets" : {
    "bundle" : true,
    "compress" : true,
    "digest" : true,
    "files" : [
      "stylesheets/html.scss"
    ]
